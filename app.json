{
  "expo": {
    "name": "Shopping App",
    "slug": "shoppingapp",
    "description": "A modern shopping app built with React Native and Expo",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "shoppingapp",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#007AFF",
    "backgroundColor": "#ffffff",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "githubUrl": "https://github.com/basanth-pedapati/shoppingapp",
    "ios": {
      "bundleIdentifier": "com.basanthpedapati.shoppingapp",
      "supportsTablet": true,
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.basanthpedapati.shoppingapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "buildToolsVersion": "34.0.0",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.SYSTEM_ALERT_WINDOW"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Shopping App",
      "shortName": "ShopApp",
      "lang": "en",
      "scope": "/",
      "themeColor": "#007AFF",
      "description": "A modern shopping app built with React Native and Expo",
      "dir": "auto",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "preferRelatedApplications": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "215ebf03-12f3-4ad8-a8b7-9f2340c71c43"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/215ebf03-12f3-4ad8-a8b7-9f2340c71c43",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "enabled": true
    },
    "owner": "basanth-pedapati",
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
